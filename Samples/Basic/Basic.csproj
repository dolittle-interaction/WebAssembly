<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="../../Build/MSBuild/default.props"/>
    
    <PropertyGroup>
        <OutputType>library</OutputType>
        <TargetFramework>netstandard2.0</TargetFramework>
        
        <!-- Optimize build time -->
        <DefaultItemExcludes>node_modules\**\*;jspm_packages\**\*;bower_components\**\*;wwwroot\**\*;bin\**\*;obj\**\*</DefaultItemExcludes>
        <DocumentationFile></DocumentationFile>
    </PropertyGroup>

    <!-- Optimize build time -->
    <ItemGroup>
      <Content Remove="node_modules\**\*" />
      <Content Remove="jspm_packages\**\*" />
      <Content Remove="bower_components\**\*" />
      <Content Remove="bin\**\*" />
      <Content Remove="obj\**\*" />
      <Content Remove="wwwroot\**\*" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Dolittle.SDK" Version="2.0.0-alpha2*"/>
        <PackageReference Include="Dolittle.DependencyInversion.Autofac" Version="2.0.0-alpha2*"/>
        <PackageReference Include="Dolittle.Interaction.WebAssembly.Build" Version="2.0.0-alpha2*" />
    </ItemGroup>
</Project>
